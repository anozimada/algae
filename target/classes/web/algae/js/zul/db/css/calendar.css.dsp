ition:relative;overflow:hidden}.z-calendar-anima-inner{width:200%;position:absolute}.z-calendar-anima-inner table{width:50%;float:left}.z-calendar-selected{color:#88a857;font-weight:700;background:#fff}.z-calendar-selected:hover{color:#fff;background:#88a857}.z-calendar-outside{color:#acacac;background:#fff}.z-datebox-popup{position:absolute}.z-datebox-popup .z-calendar+.z-timebox{margin:4px}.z-datebox-popup .z-calendar ~ .z-datebox-timezone{margin:0 4px 4px}.z-datebox-timezone{font-family:"Open Sans",Arial,Sans-serif;font-size:12px;font-weight:600;font-style:normal;color:#555}