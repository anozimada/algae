<%@ taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c" %><%@ taglib uri="http://www.zkoss.org/dsp/zk/core" prefix="z" %><%@ taglib uri="http://www.zkoss.org/dsp/web/theme" prefix="t" %>.z-button{font-family:"Open Sans",Arial,Sans-serif;font-size:12px;font-weight:600;font-style:normal;color:#fff;min-height:24px;border:1px solid #88a857;padding:3px 15px;line-height:16px;background:#88a857;cursor:pointer;white-space:nowrap}.z-button:hover{color:#fff;border-color:#95b95f;background:#95b95f;-webkit-box-shadow:inset 0 -1px 0 #6a8344;-moz-box-shadow:inset 0 -1px 0 #6a8344;-o-box-shadow:inset 0 -1px 0 #6a8344;-ms-box-shadow:inset 0 -1px 0 #6a8344;box-shadow:inset 0 -1px 0 #6a8344}.z-button:focus{color:#fff;border-color:#6a8344;background:#6a8344;-webkit-box-shadow:none;-moz-box-shadow:none;-o-box-shadow:none;-ms-box-shadow:none;box-shadow:none}.z-button:active{color:#fff;border-color:#77944c;background:#77944c;-webkit-box-shadow:inset 0 1px 0 #2196f3;-moz-box-shadow:inset 0 1px 0 #2196f3;-o-box-shadow:inset 0 1px 0 #2196f3;-ms-box-shadow:inset 0 1px 0 #2196f3;box-shadow:inset 0 1px 0 #2196f3}.z-button[disabled]{color:#acacac;border:1px solid #e3e3e3;background:#e3e3e3;opacity:1;filter:alpha(opacity=100);-webkit-box-shadow:none;-moz-box-shadow:none;-o-box-shadow:none;-ms-box-shadow:none;box-shadow:none;cursor:default}.ie8 .z-button{min-height:16px}