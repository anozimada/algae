<%@ taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c" %><%@ taglib uri="http://www.zkoss.org/dsp/zk/core" prefix="z" %><%@ taglib uri="http://www.zkoss.org/dsp/web/theme" prefix="t" %>.z-timepicker{display:inline-block;min-height:24px;line-height:normal;white-space:nowrap;clear:both}.z-timepicker-input{font-family:"Open Sans",Arial,Sans-serif;font-size:12px;font-weight:400;font-style:normal;color:#555;height:24px;border:1px solid #e3e3e3;border-right-width:0;margin:0;padding:3px 7px;line-height:16px;background:#fff}.z-timepicker-input:focus{border-color:#6a8344;-webkit-box-shadow:inset -1px 0 0 #6a8344;-moz-box-shadow:inset -1px 0 0 #6a8344;-o-box-shadow:inset -1px 0 0 #6a8344;-ms-box-shadow:inset -1px 0 0 #6a8344;box-shadow:inset -1px 0 0 #6a8344}.z-timepicker-input[readonly]{border-color:#e3e3e3;background:#f5f5f5;-webkit-box-shadow:none;-moz-box-shadow:none;-o-box-shadow:none;-ms-box-shadow:none;box-shadow:none}.z-timepicker-button{display:inline-block;width:24px;height:24px;border:1px solid #e3e3e3;background:#fff;vertical-align:middle;cursor:pointer;overflow:hidden}.z-timepicker-button:hover{border-color:#95b95f;background:#95b95f}.z-timepicker-button:hover>i{color:#fff}.z-timepicker-button:active{border-color:#77944c;background:#77944c}.z-timepicker-button:active>i{color:#fff}input[disabled]+.z-timepicker-button:hover,input[disabled]+.z-timepicker-button:active{border-color:#e3e3e3;background:0}.z-timepicker-icon{font-size:12px;color:#555;display:block!important;width:22px;line-height:22px;text-align:center}.z-timepicker-disabled{opacity:1;filter:alpha(opacity=100)}.z-timepicker-disabled *{color:#acacac!important;background:#f2f2f2!important;cursor:default!important}.z-timepicker-invalid{border:1px solid #ae312e!important;margin-right:-1px;background:#fff;-webkit-box-shadow:inset -1px 0 0 #ae312e;-moz-box-shadow:inset -1px 0 0 #ae312e;-o-box-shadow:inset -1px 0 0 #ae312e;-ms-box-shadow:inset -1px 0 0 #ae312e;box-shadow:inset -1px 0 0 #ae312e}.z-timepicker-readonly:focus{border-right-width:0;border-color:#e3e3e3;background:transparent repeat-x 0 0;cursor:default}.z-timepicker-rightedge{border-right:1px solid #e3e3e3}.z-timepicker-button{font-size:13px}.z-timepicker-inplace .z-timepicker-input{padding:5px;border:0;background:0}.z-timepicker-inplace .z-timepicker-input:focus{background:0}.z-timepicker-inplace .z-timepicker-button{display:none}.z-timepicker-popup{max-height:200px}.z-timepicker-option{display:block;padding:4px 8px;line-height:16px;position:relative;white-space:nowrap;cursor:pointer;color:#555;text-decoration:none}.z-timepicker-option:hover{color:#fff;background:#95b95f}.z-timepicker-option:hover .z-timepicker-option-inner,.z-timepicker-option:hover .z-timepicker-option-content{color:#fff}.z-timepicker-popup{font-family:"Open Sans",Arial,Sans-serif;font-size:12px;font-weight:400;font-style:normal;color:#555;display:block;border:1px solid #e3e3e3;background:#fff;position:absolute;overflow:auto}.z-timepicker-content{border:0;margin:0;padding:0;background:transparent none repeat 0 0;position:relative;list-style-image:none;list-style-position:outside;list-style-type:none}